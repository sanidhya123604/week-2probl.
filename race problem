//adveta and his friend race//
n  = int(input())
k=0
for n in range(1,n+1):
    j=100
    for n in range(3,n+1):
        j=j+10
    if  n>1:
        j=j+10
    k=k+j
print(k)
k=0
for n in range(1,n+1):
    j=120
    for n in range(3,n+1):
        j=j+5
    if n>1:
        j=j+5
    k=k+j

print(k)
k=0
for n in range(1,n+1):
    j=80
    for n in range(3,n+1):
        j=j+20
    if n>1:
        j=j+20
    k=k+j
print(k)
